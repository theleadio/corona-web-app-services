{
  "name": "corona-services",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn copy-static",
    "copy-static": "cp -rv src/static/ dist/static/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start && yarn copy-static",
    "start:dev": "nest start --watch && yarn copy-static",
    "start:debug": "nest start --debug --watch && yarn copy-static",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "webpack": "webpack --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "update:orm:ct": "typeorm-model-generator -h coronatracker.coehycitad7u.ap-southeast-1.rds.amazonaws.com -d coronatracker -u corona -x corona0106 -e mysql -o ./src/db/coronatracker && rm ./src/db/coronatracker/ormconfig.json ./src/db/coronatracker/tsconfig.json"
  },
  "dependencies": {
    "@google-cloud/automl": "^1.7.3",
    "@google-cloud/translate": "^5.0.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/microservices": "^6.8.3",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/platform-socket.io": "^6.8.3",
    "@nestjs/swagger": "^4.0.9",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.8.3",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/redis": "^2.8.14",
    "dotenv": "^8.2.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.17.1",
    "pg": "^7.12.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.1",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typeorm-model-generator": "^0.3.5",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
